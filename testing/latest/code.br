// // Test Stack Types
import wasm print: (Number) -> Void from 'env';
let loop: Function = (body: Function): Void => {
  let _loop: Function = (body: Function, index: Number): Number => {
    if (body(index)) return(0);
    _loop(body, index);
    return(1);
  };
  _loop(body, 0);
};
loop((index: Number): Boolean => {
  print(index);
  return(true);
});
