{
  "name": "brisk-cli",
  "version": "0.1.2",
  "description": "Brisk Command Line Interface",
  "main": "./cli/index.ts",
  "author": "spotandjake",
  "license": "Apache-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/spotandjake/Brisk.git"
  },
  "pkg": {
    "scripts": "./dist/brisk.js",
    "targets": [
      "node15"
    ],
    "outputPath": "/"
  },
  "scripts": {
    "start": "yarn build && yarn run run",
    "build": "gulp build",
    "run": "node --enable-source-maps ./dist/brisk.js",
    "package": "gulp package",
    "start-tests": "yarn build-tests && yarn run-tests",
    "build-tests": "gulp build-tests",
    "run-tests": "node --enable-source-maps ./dist/brisk-tests.js",
    "lint": "gulp lint"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "binaryen": "^101.0.0",
    "gulp": "^4.0.2",
    "gulp-eslint": "^6.0.0",
    "nearley": "^2.20.1",
    "pkg": "^5.3.0",
    "rollup": "^2.56.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/nearley": "^2.11.1",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "typescript": "^4.3.5"
  }
}
